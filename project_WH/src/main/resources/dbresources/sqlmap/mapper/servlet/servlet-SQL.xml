<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="servlet">

	<select id="serVletTest" resultType="EgovMap">
		<![CDATA[
			SELECT 
				FILENAME, FILEREALNAME
			FROM
				UPLOAD
			WHERE
				1=1
		]]>
	</select>
	
	<select id="sidonm" resultType="ServletVO">
	SELECT DISTINCT sidonm FROM public."TB_CARBON_E2"
	</select>
	
	<select id="sgg_nm" resultType="ServletVO" parameterType="String">
	SELECT DISTINCT sgg_nm FROM public."tl_sgg" WHERE sd_nm = #{loc1}
	</select>
	
	<select id="bjd_nm" resultType="ServletVO">
	SELECT DISTINCT bjd_nm FROM public."tl_bjd"
	</select>
</mapper>